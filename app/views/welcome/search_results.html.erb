<div >
  <%= form_tag("/search", method: "get") do %>
  <%= label_tag(:name, "Search for another Twitter user: @",) %>
  <%= text_field_tag(:name, nil, ) %>
  <%= submit_tag("Search") %>
  <% end %>      
</div>

<br>
<br>

<%= link_to @name, "https://twitter.com/#{@name}",target: "blank" %>  joined Twitter on <%= @created_at.strftime("%A %B %d, %Y") %>
<br>
<br>
User id: <%= "#{@user_id}" %>


